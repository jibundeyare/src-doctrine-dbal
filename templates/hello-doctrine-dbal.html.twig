<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
</head>
<body>

    {# parcours de chacun des éléments du tableau `$students` #}
    {% for student in students %}
        {# à chaque itération de la boucle, la variable `$student` contient une ligne de la table #}
        {# chaque clé alpha-numérique représente une colonne de la table #}
        {{ student.id }}<br />
        {{ student.firstname }}<br />
        {{ student.lastname }}<br />
        <br />
    {% endfor %}

    {% for project in projects %}
        {{ project.id }}<br />
        {{ project.name }}<br />
        {{ project.description }}<br />
        <br />
    {% endfor %}

    {% for studentWithProject in studentsWithProject %}
        {{ studentWithProject.student_id }}<br /> {# id du student #}
        {{ studentWithProject.firstname }}<br />
        {{ studentWithProject.lastname }}<br />
        {{ studentWithProject.project_id }}<br /> {# id du projet #}
        {{ studentWithProject.name }}<br />
        {{ studentWithProject.description }}<br />
    {% endfor %}

</body>
</html>